{
  "name": "angel-bridge",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "web": "yarn workspace web dev",
    "server": "yarn workspace server dev",
    "dev": "yarn workspaces foreach -vip run dev",
    "build": "yarn workspaces foreach -vit run build",
    "format": "yarn workspaces foreach -vi run format",
    "lint": "yarn workspaces foreach -vi run lint"
  },
  "packageManager": "yarn@3.2.3"
}
